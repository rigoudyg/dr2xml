      },
      "further_info_url": {
        "skip_values": ["", "None"]
      },
      "history": {
        "default_values": ["history!:@simulation", "none"]
      },
      "initialization_index": {
        "default_values": ["initialization_index!:@simulation", "1"],
        "num_type": "int"
      },
      "institution_id": {
        "default_values": ["institution_id!:@lset"],
        "fatal": "True"
      },
      "parent_experiment_id": {
        "conditions": ["key:parent_experiment_id!=val:no parent", "key:parent_experiment_id!=val:",
          "key:parent_experiment_id!=val:None"]
      },
      "parent_mip_era": {
        "conditions": ["key:parent_experiment_id!=val:no parent", "key:parent_experiment_id!=val:",
          "key:parent_experiment_id!=val:None"],
        "default_values": ["parent_mip_era!:@simulation", "mip_era!:@dict"]
      },
      "parent_activity_id": {
        "conditions": ["key:parent_experiment_id!=val:no parent", "key:parent_experiment_id!=val:",
          "key:parent_experiment_id!=val:None"]
      },
      "parent_source_id": {
        "conditions": ["key:parent_experiment_id!=val:no parent", "key:parent_experiment_id!=val:",
          "key:parent_experiment_id!=val:None"],
        "default_values": ["parent_source_id!:@simulation", "source_id!:@dict"]
      },
      "parent_time_units": {
        "conditions": ["key:parent_experiment_id!=val:no parent", "key:parent_experiment_id!=val:",
          "key:parent_experiment_id!=val:None"],
        "default_values": ["parent_time_units!:@simulation",
          "parent_time_ref_year!:@simulation!:@days since {}-01-01 00:00:00", "days since 1850-01-01 00:00:00"]
      },
      "parent_variant_label": {
        "conditions": ["key:parent_experiment_id!=val:no parent", "key:parent_experiment_id!=val:",
          "key:parent_experiment_id!=val:None"],
        "default_values": ["parent_variant_label!:@simulation", "variant_label!:@dict"]
      },
      "branch_time_in_parent": {
        "conditions": ["key:parent_experiment_id!=val:no parent", "key:parent_experiment_id!=val:",
          "key:parent_experiment_id!=val:None"],
        "num_type": "double",
        "skip_values": ["", "None"]
      },
      "branch_time_in_child": {
        "conditions": ["key:parent_experiment_id!=val:no parent", "key:parent_experiment_id!=val:",
          "key:parent_experiment_id!=val:None"],
        "num_type": "double",
        "skip_values": ["", "None"]
      },
      "physics_index": {
        "default_values": ["physics_index!:@simulation", "1"],
        "num_type": "int"
      },
      "product": {
        "default_values": ["model-output"]
      },
      "realization_index": {
        "default_values": ["realization_index!:@simulation", "1"],
        "num_type": "int"
      },
      "references": {
        "default_values": ["references!:@laboratory"]
      },
      "sub_experiment_id": {
        "default_values": ["sub_experiment_id!:@simulation", "none"]
      },
      "sub_experiment": {
        "default_values": ["sub_experiment!:@simulation", "none"]
      },
      "variant_info": {
        "default_values": ["variant_info!:@simulation!:@{}. Information provided by this attribute may in some cases be flawed. Users can find more comprehensive and up-to-date documentation via the further_info_url global attribute.", "none"],
        "skip_values": ["none", ". Information provided by this attribute may in some cases be flawed. Users can find more comprehensive and up-to-date documentation via the further_info_url global attribute."]
      }
    }
