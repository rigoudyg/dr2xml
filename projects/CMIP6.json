    "vars_constraints": {
      "contact": {
        "default_values": ["contact!:@simulation", "contact!:@laboratory", "None"],
        "skip_values": ["None", ""]
      },
      "data_specs_version": {
        "default_values": ["!:@DR_version"]
      },
      "dr2xml_version": {
        "default_values": ["version!:@config"]
      },
      "expid_in_filename": {
        "output_key": "experiment_id"
      },
      "description": {
        "skip_values": ["", "None"],
        "conditions": ["experiment_id!=expid_in_filename"]
      },
      "title_desc": {
        "skip_values": ["", "None"],
        "output_key": "title",
        "conditions": ["key:experiment_id!=key:expid_in_filename"]
      },
      "experiment": {
        "skip_values": ["", "None"],
        "conditions": ["key:experiment_id!=key:expid_in_filename"]
      },
      "external_variables": {
        "skip_values": [""]
      },
      "forcing_index": {
        "default_values": ["forcing_index!:@simulation", "1"],
