		<field id="CMIP7_siareaacrossline" field_ref="?dummy_TRS"/>
		<!--P1 (m2 s-1) sea_ice_area_transport_across_line : Flux across 4 straits-->
		<field id="CMIP7_siarean" field_ref="?dummy_0d"/>
		<!--P1 (1e6 km2) sea_ice_area : total area of sea ice in the Northern hemisphere-->
		<field id="CMIP7_siareas" field_ref="?dummy_0d"/>
		<!--P1 (1e6 km2) sea_ice_area : total area of sea ice in the Southern hemisphere-->
		<field id="CMIP7_sichl" field_ref="?dummy_XY"/>
