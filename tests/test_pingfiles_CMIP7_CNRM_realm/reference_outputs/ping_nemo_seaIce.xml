		<field id="CMIP7_simpthick" field_ref="?dummy_XY"/>
		<!--P1 (m) sea_ice_melt_pond_thickness : Meltpond Depth-->
		<field id="CMIP7_sino3" field_ref="?dummy_XY"/>
		<!--P1 (mol m-2) sea_ice_mole_content_of_nitrate : Mole concentration means moles (amount of substance) per unit area-->
		<field id="CMIP7_sipr" field_ref="?dummy_XY"/>
		<!--P1 (kg m-2 s-1) rainfall_flux : mass of liquid precipitation falling onto sea ice divided by grid-cell area-->
		<field id="CMIP7_sirdgconc" field_ref="?dummy_XY"/>
